    "mud:up": "pnpm mud set-version --tag main && pnpm install"
  },
  "devDependencies": {
    "@types/debug": "4.1.7",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "eslint": "8.57.0",
    "rimraf": "^3.0.2",
    "typescript": "5.4.2"
  },
  "engines": {
    "node": "^18",
    "pnpm": "^8 || ^9"
  }
}